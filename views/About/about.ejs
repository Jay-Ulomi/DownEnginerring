<!-- Page Title -->
<section class="page-title">
    <div class="content" style="background-image: url(assets/images/background/15.png)">
        <div class="auto-container">
            <h1>About us</h1>
        </div>
    </div>
    <ul class="page-breadcrumb">
        <li><a href="/">home</a></li>
        <li id="about-us">About us</li>
    </ul>
</section>
<!-- End Page Title -->

<!-- Innovation Section Two / Style Two -->
<section class="experiance-section-two style-two">
	<div class="auto-container">
		<div class="row clearfix">
			<!-- Content Column -->
			<div class="content-column col-lg-6 col-md-12 col-sm-12">
				<div class="inner-column">
					<div class="sec-title">
						<div class="title">Engineering Excellence Since 2024</div>
						<h2>Advancing <span>Telecom & Infrastructure</span> <br> with Smart Solutions</h2>
						<div class="text">Dowelef Engineering is a leader in telecom infrastructure, fiber optic
							networks, and civil engineering. We focus on cutting-edge solutions, reliability, and
							sustainable growth to empower industries across Africa.</div>
					</div>
					<!-- Fact Counter / Style Three -->
					<div class="fact-counter style-three">
						<div class="row clearfix">
							<!-- Column -->
							<div class="column counter-column col-lg-6 col-md-6 col-sm-12">
								<div class="inner wow fadeInLeft" data-wow-delay="300ms" data-wow-duration="1500ms">
									<div class="content">
										<div class="icon flaticon-helmet"></div>
										<div class="count-outer count-box alternate">
											<span class="count-text" data-speed="5000" data-stop="1600">0</span>+
										</div>
										<h4 class="counter-title">Telecom Towers Built</h4>
									</div>
								</div>
							</div>

							<!-- Column -->
							<div class="column counter-column col-lg-6 col-md-6 col-sm-12">
								<div class="inner wow fadeInLeft" data-wow-delay="600ms" data-wow-duration="1500ms">
									<div class="content">
										<div class="icon flaticon-blueprint"></div>
										<div class="count-outer count-box">
											<span class="count-text" data-speed="3000" data-stop="1500">0</span> km
										</div>
										<h4 class="counter-title">Fiber Optic Network Installed</h4>
									</div>
								</div>
							</div>
						</div>
					</div>

					<!-- Button Box -->
					<div class="button-box">
						<a class="btn-style-three theme-btn" href="/services"><span class="txt">Explore
								Services</span></a>
					</div>
				</div>
			</div>

			<!-- Image Column -->
			<div class="image-column col-lg-6 col-md-12 col-sm-12">
				<div class="inner-column">
					<div class="year-box">
						<div class="count-box">
							0<span class="count-text" data-speed="5000" data-stop="14">0</span>
						</div>
						<span class="years">Years</span>
					</div>
					<div class="image">
						<img src="assets/images/resource/about.png" alt="Dowelef Engineering Innovation" />
					</div>
				</div>
			</div>
		</div>
	</div>
</section>
<!-- End Innovation Section Two -->

